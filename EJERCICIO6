let inp, button;

function setup() {
  createCanvas(300, 300);
  background(220);

  inp = createInput("Numero");
  inp.position(70, 65);


  button = createButton('Enviar');
  button.position(125, 120);
  button.mousePressed(fib);
}

function fib(n) {
  n = int(inp.value());
  let arr = [0, 1];
  for (let i = 2; i < n + 1; i++) {
    arr.push(arr[i - 2] + arr[i - 1])
  }

  print(arr[n]);
}
